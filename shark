connection() {
while :
do
if curl -s --head  --request GET www.google.com | grep "200 OK" > /dev/null ; then
    echo -e "\e[92m-->> Active Internet Connection\e[m"
    echo 
    sleep 1
    echo -e "\e[92m-->> READY TO GO...\e[m"
    sleep 2
    echo
    break
else
    echo -e "\e[91m-->> Inactive Internet Connection\e[m"
    sleep 1
    echo
    echo -e "\e[93m-->> Check your connection and Try Again!!\e[m"
    sleep 2
    echo
    exit
fi
done
